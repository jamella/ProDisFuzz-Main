<?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.layout.HBox?><?import java.net.URL?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="javafx.scene.layout.HBox" id="controlBar">
    <children>
        <Label fx:id="memoryUsage" maxWidth="Infinity" HBox.hgrow="ALWAYS" maxHeight="Infinity" />
        <Button fx:id="cancelButton" cancelButton="true" text="Cancel" onAction="#cancel" />
        <Button fx:id="nextButton" defaultButton="true" text="Next >" onAction="#next" />
        <Button fx:id="finishButton" text="Finish" onAction="#finish" />
    </children>
    <stylesheets>
        <URL value="@../css/controlBar.css" />
    </stylesheets>
</fx:root>
