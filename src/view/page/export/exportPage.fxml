<?import javafx.geometry.Insets?><?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.TextField?><?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="javafx.scene.layout.VBox" spacing="20">
    <children>
        <GridPane vgap="10" hgap="10">
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" />
                <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints hgrow="NEVER" />
            </columnConstraints>
            <Label text="Export to:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <TextField fx:id="fileTextField" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="0"
                       maxWidth="Infinity" />
            <Button fx:id="browseButton" defaultButton="false" onAction="#browse" text="Browse..."
                    GridPane.columnIndex="2" GridPane.rowIndex="0" />
        </GridPane>
    </children>
    <padding>
        <Insets bottom="0" left="10" right="10" top="10" />
    </padding>
</fx:root>
